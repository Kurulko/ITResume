@page "/programming-languages"
@inherits _AdminLayout

<CascadingValue Value="50" Name="PageSize">
    <SimpleEditITResumeDbModels Service="ProgrammingLanguageService" TModel="ProgrammingLanguage">
        <Columns>
            <GridColumn Field=@nameof(ProgrammingLanguage.Name)
                        FilterSettings="@(new FilterSettings() { Operator = Operator.Contains })" />
        </Columns>
    </SimpleEditITResumeDbModels>
</CascadingValue>

@code {
    [Inject]
    public IProgrammingLanguageService ProgrammingLanguageService { get; set; } = null!;
}
