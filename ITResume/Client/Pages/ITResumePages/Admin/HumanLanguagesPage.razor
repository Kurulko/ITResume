@page "/human-languages"
@inherits _AdminLayout

<CascadingValue Value="50" Name="PageSize">
    <SimpleEditITResumeDbModels Service="HumanLanguageService" TModel="HumanLanguage">
        <GridColumns>

            <GridColumn Field=@nameof(HumanLanguage.Id) AllowAdding="false" />
            <GridColumn Field=@nameof(HumanLanguage.Name)
                        FilterSettings="@(new FilterSettings() { Operator = Operator.Contains })" />
        </GridColumns>
    </SimpleEditITResumeDbModels>
</CascadingValue>

@code {
    [Inject]
    public IHumanLanguageService HumanLanguageService { get; set; } = null!;
}
